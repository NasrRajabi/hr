<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
               <!-- Handlers for PHP via FastCGI -->
        <handlers>
            <add name="PHP via FastCGI" path="*.php" verb="*" modules="FastCgiModule" scriptProcessor="C:\PHP\php-cgi.exe" resourceType="File" />
        </handlers>
    </system.webServer>
</configuration>
